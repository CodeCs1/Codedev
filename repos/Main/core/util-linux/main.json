{
    "package":"util-linux",
    "version":"2.40.1",
    "URL":[
        "https://www.kernel.org/pub/linux/utils/util-linux/v2.40/util-linux-2.40.1.tar.xz"
    ],
    "file":[
        "util-linux-2.40.1.tar.xz"
    ],
    "PostBuild":[
        "./configure --bindir=/usr/bin --libdir=/usr/lib --runstatedir=/run --sbindir=/usr/sbin   --disable-chfn-chsh   --disable-login       --disable-nologin     --disable-su          --disable-setpriv     --disable-runuser     --disable-pylibmount  --disable-liblastlog2 --disable-static      --without-python      --without-systemd     --without-systemdsystemunitdir        ADJTIME_PATH=/var/lib/hwclock/adjtime --docdir=/usr/share/doc/util-linux-2.40.1"
    ],
    "Build": [
        "make"
    ],
    "enabletest":false,
    "Install":[
        "make install"
    ]
}