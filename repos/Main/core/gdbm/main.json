{
    "package":"gdbm",
    "version":"1.23",
    "URL":[
        "https://ftp.gnu.org/gnu/gdbm/gdbm-1.23.tar.gz"
    ],
    "file":[
        "gdbm-1.23.tar.gz"
    ],
    "PostBuild":[
        "./configure --prefix=/usr --disable-static --enable-libgdbm-compat"
    ],
    "Build": [
        "make"
    ],
    "enabletest":true,
    "Test":[
        "make check"
    ],
    "Install":[
        "make install"
    ]
}