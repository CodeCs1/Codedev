{
    "package":"mpc",
    "version":"1.3.1",
    "URL":[
        "https://ftp.gnu.org/gnu/mpc/mpc-1.3.1.tar.gz"
    ],
    "file":[
        "mpc-1.3.1.tar.gz"
    ],
    "PostBuild":[
        "./configure --prefix=/usr --disable-static --docdir=/usr/share/doc/mpc-1.3.1"
    ],
    "Build": [
        "make",
        "make html"
    ],
    "enabletest":true,
    "Test":[
        "make check"
    ],
    "Install":[
        "make install",
        "make install-html"
    ]
}